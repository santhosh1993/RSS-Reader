# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'RSS-Reader' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RSS-Reader
	pod 'Firebase/Analytics'
	pod 'RSSFeederLogin' , :git => 'https://github.com/santhosh1993/RSSFeederLogin.git' ,:tag => "0.0.4"
  	pod 'RSSDataLoader' , :git => 'https://github.com/santhosh1993/RSSDataLoader.git' ,:tag => "0.0.2"
	pod 'Notes' , :git => 'https://github.com/santhosh1993/Notes.git' ,:tag => "0.0.3"

  target 'RSS-ReaderTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RSS-ReaderUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
